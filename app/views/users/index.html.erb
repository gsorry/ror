<h1>Users</h1>

<%= link_to 'New User', new_user_path %>

<table>
  <thead>
  <tr>
    <th>#</th>
    <th>Image</th>
    <th>Email</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th></th>
  </tr>
  </thead>

  <tbody>
    <% cache @users do %>
      <% @users.each do |user| %>
        <%= render user, cached: true %>
      <% end %>
    <% end %>
  </tbody>
</table>
